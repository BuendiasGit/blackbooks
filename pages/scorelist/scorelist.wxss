/* pages/scorelist/scorelist.wxss */
page{
  height: 100%;
  background-color: rgb(250,250,250);
}
.header{
  width: 96%;
  margin:20rpx auto;
  border-radius: 20rpx;
  padding-top: 30rpx;
  padding-bottom: 30rpx; 
  display: flex;
  font-size: 40rpx;
  text-align: center;
  background-color: rgb(240,240,240);
}
.msgbody{
  margin-top: 20rpx;
}
.msg{
  background-color: white;
  padding: 30rpx 0 30rpx 0;
  border-bottom: 4rpx solid rgb(245,245,245);
  display: flex;
  font-size: 30rpx;
}
.h1{
  margin: 0 10rpx 0 40rpx;
}
.h12{
  margin: 0 10rpx 0 100rpx;
}
.h2{
  margin: 0 10rpx 0 80rpx;
}
.h22{
  margin: 0 10rpx 0 120rpx;
}